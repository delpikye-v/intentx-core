{
  "name": "intentx-core-z",
  "version": "2.1.0",
  "description": "High-performance intent-driven execution engine. Orchestrates business logic via deterministic routing, managed effects, and a reactive dependency graph.",
  "license": "MIT",
  "author": "Delpi.Kye",
  "sideEffects": false,
  "type": "module",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.esm.js",
      "require": "./build/index.cjs.js"
    },
    "./react": {
      "types": "./build/react/index.d.ts",
      "import": "./build/react/index.esm.js",
      "require": "./build/react/index.cjs.js"
    },
    "./devtools": {
      "types": "./build/devtools/index.d.ts",
      "import": "./build/devtools/index.esm.js",
      "require": "./build/devtools/index.cjs.js"
    }
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf build",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "cb": "npm run clean && npm run build",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run cb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delpikye-v/intentx-core.git"
  },
  "homepage": "https://github.com/delpikye-v/intentx-core#readme",
  "bugs": {
    "url": "https://github.com/delpikye-v/intentx-core/issues"
  },
  "keywords": [
    "intent-driven",
    "logic-engine",
    "reactive-graph",
    "state-orchestration",
    "headless",
    "deterministic",
    "async-effects",
    "backend-agnostic",
    "domain-driven-design",
    "event-loop",
    "typescript"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "eventbus-z": "^2.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}